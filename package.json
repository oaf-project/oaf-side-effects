    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "type-coverage": "^2.21.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts --report-unused-disable-directives",
