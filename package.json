    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/experimental-utils": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "codecov": "^3.8.3",
    "eslint": "^8.9.0",
