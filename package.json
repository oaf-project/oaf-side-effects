{
  "name": "oaf-side-effects",
  "version": "3.1.0",
  "main": "dist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oaf-project/oaf-side-effects.git"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.3.1",
    "@stryker-mutator/jest-runner": "^3.3.1",
    "@stryker-mutator/typescript": "^3.3.1",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/experimental-utils": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "codecov": "^3.7.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typed-fp": "^0.9.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-total-functions": "^3.1.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "type-coverage": "^2.13.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "type-coverage": "type-coverage --strict --detail --ignore-files \"src/index.test.ts\"",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "release": "yarn build && yarn lint && yarn type-coverage --strict --ignore-catch --detail && yarn test && yarn publish && yarn documentation",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100
  },
  "dependencies": {
    "type-zoo": "^3.4.1"
  }
}
